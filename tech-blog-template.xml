            <b:defaultmarkup type='Header'>
                <b:includable id='main' var='this'>
                    <div class='header-widget'>
                        <b:include cond='data:imagePlacement in {&quot;REPLACE&quot;, &quot;BEFORE_DESCRIPTION&quot;}' name='image' />
                        <div class='row'>
                            <div class='col-12 col-lg-9'>
                                <div class='row'>
                                    <div class='col-12 col-lg-2' />
                                    <div class='col-12 col-lg-10'>
                                        <div class='k-header-contents mt-1 mb-1 pr-1 pl-1'>
                                            <b:include cond='data:imagePlacement not in {&quot;REPLACE&quot;, &quot;BEFORE_DESCRIPTION&quot;}' name='title' />
                                            <b:include cond='data:imagePlacement != &quot;REPLACE&quot;' name='description' />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class='col-12 col-lg-3' />
                        </div>
                        <b:include cond='data:imagePlacement == &quot;BEHIND&quot;' name='behindImageStyle' />
                        <b:if cond='data:sourceUrl'>
                            <style>
                                .k-header-contents {
                                    border-radius: 8px;
                                    background: rgba(255,255,255,0.5);
                                }
                            </style>
                        </b:if>
                    </div>
                </b:includable>
                <b:includable id='title'>
                    <div class='k-header-title text-dark h3 p-1'>
                        <data:title />
                    </div>
                </b:includable>
                <b:includable id='description'>
                    <b:if cond='data:view.isHomepage'>
                        <div class='k-header-description text-dark p-1'>
                            <data:description />
                        </div>
                    </b:if>
                </b:includable>
            </b:defaultmarkup>
            <b:defaultmarkup type='Blog'>
                <b:includable id='main'>
                    <div class='row'>
                        <div class='col-12 col-lg-2'>
                            <b:with value='data:posts.first' var='post'>
                                <div class='text-left text-lg-center row'>
                                    <b:include cond='data:post.shareUrl' data='post' name='shareButtons' />
                                    <b:include cond='data:feedLinks' name='feedLinks' />
                                </div>
                            </b:with>
                        </div>
                        <div class='col-12 col-lg-10'>
                            <b:if cond='data:blog.pageType in {&quot;item&quot;, &quot;static_page&quot;}'>
                                <b:with value='data:posts.first' var='post'>
                                    <article class='k-item'>
                                        <div class='k-item-header'>
                                            <h3 class='k-item-header-title k-wrap'>
                                                <data:post.title />
                                            </h3>
                                            <div class='d-flex'>
                                                <b:include name='commentNum' />
                                                <b:include name='postDate' />
                                            </div>
                                            <b:if cond='data:blog.pageType == &quot;item&quot;'>
                                                <b:if cond='data:post.labels'>
                                                    <p class='k-item-labels'>
                                                        <b:loop values='data:post.labels' var='label'>
                                                            <a expr:href='data:label.url' class='badge badge-primary'>
                                                                <data:label.name />
                                                            </a>
                                                        </b:loop>
                                                    </p>
                                                </b:if>
                                            </b:if>
                                        </div>
                                        <nav class='js-toc toc navbar navbar-light bg-light d-block d-lg-none'></nav>
                                        <div class='k-item-body js-toc-content'>
                                            <data:post.body />
                                        </div>
                                    </article>
                                </b:with>
                                <b:else />
                                <main class='k-index'>
                                    <b:loop values='data:posts' var='post'>
                                        <div class='k-index-item card mb-3'>
                                            <div class="card-body">
                                                <div class='card-title'>
                                                    <h3>
                                                        <data:post.title />
                                                    </h3>
                                                    <div class='d-flex'>
                                                        <b:include name='commentNum' />
                                                        <b:include name='postDate' />
                                                    </div>
                                                    <p>
                                                        <b:loop values='data:post.labels' var='label'>
                                                            <span class='badge badge-secondary small'>
                                                                <data:label.name />
                                                            </span>
                                                        </b:loop>
                                                    </p>
                                                </div>
                                                <p class="card-text small">
                                                    <data:post.snippets.short />
                                                </p>
                                                <a expr:href='data:post.url' class='stretched-link'></a>
                                            </div>
                                        </div>
                                    </b:loop>
                                </main>
                            </b:if>
                        </div>
                        <div class='col-12 col-lg-2 pb-2' />
                        <div class='col-12 col-lg-10 pb-2'>
                            <div class='row'>
                                <div class='col-6 text-left'>
                                    <b:include cond='data:olderPageUrl' name='previousPageLink' />
                                </div>
                                <div class='col-6 text-right'>
                                    <b:include cond='data:newerPageUrl' name='nextPageLink' />
                                </div>
                            </div>
                        </div>
                        <div class='col-12 col-lg-2' />
                        <div class='col-12 col-lg-10'>
                            <div class='k-item-comment mt-6'>
                                <b:if cond='data:blog.pageType == &quot;item&quot;'>
                                    <b:loop values='data:posts' var='post'>
                                        <b:if cond='data:post.allowComments'>
                                            <b:include data='post' name='commentPicker' />
                                        </b:if>
                                    </b:loop>
                                </b:if>
                            </div>
                        </div>
                    </div>
                </b:includable>
                <b:includable id='commentNum'>
                    <b:if cond='data:post.numberOfComments&gt;0'>
                        <div class='mr-auto text-secondary small'>
                            <i class="far fa-comment-dots"></i>
                            <b:message name='messages.numberOfComments'>
                                <b:param expr:value='data:post.numberOfComments' name='numComments' />
                            </b:message>
                        </div>
                    </b:if>
                </b:includable>
                <b:includable id='postDate'>
                    <div class='ml-auto text-secondary small'>
                        <i aria-hidden='true' class="far fa-calendar-alt" style='margin-left:0.5em'></i>
                        <b:eval expr='data:post.date format &quot;yyyy-MM-dd&quot;' />
                        <b:if cond='data:post.date != data:post.lastUpdated'>
                            <i aria-hidden='true' class="fas fa-sync-alt" style='margin-left:0.5em'></i>
                            <b:eval expr='data:post.lastUpdated format &quot;yyyy-MM-dd&quot;' />
                        </b:if>
                    </div>
                </b:includable>
                <b:includable id='nextPageLink'>
                    <a expr:href='data:newerPageUrl' expr:id='data:widget.instanceId + &quot;_blog-pager-newer-link&quot;' expr:title='data:messages.newerPosts'>
                        <i aria-hidden='true' class="fas fa-caret-square-right h3"></i>
                    </a>
                </b:includable>
                <b:includable id='previousPageLink'>
                    <a expr:href='data:olderPageUrl' expr:id='data:widget.instanceId + &quot;_blog-pager-older-link&quot;' expr:title='data:messages.olderPosts'>
                        <i aria-hidden='true' class="fas fa-caret-square-left h3"></i>
                    </a>
                </b:includable>
            </b:defaultmarkup>
            <b:defaultmarkup type='BlogSearch'>
                <b:includable id='main'>
                    <b:include name='searchForm' />
                </b:includable>
                <b:includable id='searchForm'>
                    <form id='k-search-form' expr:action='data:blog.searchUrl'>
                        <b:attr cond='not data:view.isPreview' name='target' value='_top' />
                        <b:include name='urlParamsAsFormInput' />
                        <input class='k-search-text' type='text' autocomplete='off' expr:aria-label='data:messages.searchThisBlog' expr:placeholder='data:messages.searchThisBlog' expr:value='data:view.isSearch ? data:view.search.query.escaped : &quot;&quot;' name='q' />
                    </form>
                    <i aria-hidden='true' class="fa fa-search text-primary k-search-button" />
                </b:includable>
            </b:defaultmarkup>
            <b:defaultmarkup type='Label'>
                <b:includable id='main'>
                    <b:include name='widget-title' />
                    <b:with value='data:labels' var='labels'>
                        <b:include expr:name='data:display' />
                    </b:with>
                </b:includable>
                <b:includable id='labelUrl'>
                    <b:attr expr:value='data:label.url' expr:name='data:isHref ? "href" : "value"' />
                </b:includable>
                <b:includable id='list'>
                    <nav class='nav flex-column'>
                        <b:loop values='data:labels' var='label'>
                            <b:with value='data:view.search.label.escaped == data:label.name' var='isCurrent'>
                                <b:tag expr:name='data:isCurrent ? "span" : "a"'>
                                    <b:attr expr:value='data:isCurrent ? "nav-item mw-100 text-truncate disabled" : "nav-item mw-100 text-truncate"' name='class' />
                                    <b:if cond='data:showFreqNumbers'>
                                        <span>
                                            <b:attr expr:value='data:isCurrent ? "badge badge-pill badge-secondary" : "badge badge-pill badge-primary"' name='class' />
                                            <data:label.count />
                                        </span>
                                    </b:if>
                                    <b:include cond='not data:isCurrent' data='{ isHref: true }' name='labelUrl' />
                                    <data:label.name />
                                </b:tag>
                            </b:with>
                        </b:loop>
                    </nav>
                </b:includable>
                <b:includable id='cloud'>
                    <nav>
                        <b:loop values='data:labels' var='label'>
                            <b:with value='data:view.search.label.escaped == data:label.name' var='isCurrent'>
                                <b:tag expr:name='data:isCurrent ? "span" : "a"'>
                                    <b:include cond='not data:isCurrent' data='{ isHref: true }' name='labelUrl' />
                                    <b:attr expr:value='data:isCurrent ? "badge badge-secondary text-truncate mw-100 mb-1" : "badge badge-primary text-truncate mw-100 mb-1"' name='class' />
                                    <b:if cond='data:showFreqNumbers'>
                                        (<data:label.count />)
                                    </b:if>
                                    <data:label.name />
                                </b:tag>
                            </b:with>
                        </b:loop>
                    </nav>
                </b:includable>
            </b:defaultmarkup>
        </b:defaultmarkups>
    </head>

    <body>
        <div class='container-fluid'>
            <div class='row pb-1'>
                <header class='k-header-outer col-12'>
                    <b:section id='PageHeader' showaddelement='yes'>
                        <b:widget id='Header1' locked='false' title='ヘッダー' type='Header' visible='true' />
                    </b:section>
                </header>
            </div>
            <div class='row'>
                <div class='col-12 col-lg-9'>
                    <div class='row'>
                        <div class='col-12 col-lg-2' />
                        <nav class='col-12 col-lg-10'>
                            <b:section id='PostHeader' showaddelement='no'>
                                <b:widget id='HTML1' locked='true' title='BreadCrumb' type='HTML' visible='true'>
                                    <b:includable id='main'>
                                        <b:include name='BreadCrumb' />
                                    </b:includable>
                                </b:widget>
                                <b:widget id='BlogSearch1' locked='true' title='このブログを検索' type='BlogSearch' visible='true' />
                            </b:section>
                        </nav>
                    </div>
                    <b:section id='Post' showaddelement='no'>
                        <b:widget id='Blog1' locked='true' title='投稿' type='Blog' visible='true' />
                    </b:section>
                </div>
                <aside class='k-side-outer col-12 col-lg-3'>
                    <b:section id='Side1' showaddelement='yes'>
                        <b:widget id='Label1' locked='false' title='ラベル' type='Label' visible='true' />
                    </b:section>
                    <b:if cond='data:view.isSingleItem'>
                        <nav class='js-toc-lg toc navbar navbar-light bg-light sticky-top d-none d-lg-block'></nav>
                    </b:if>
                    <b:section id='Side2' showaddelement='yes' />
                </aside>
            </div>
            <div class='row'>
                <footer class='k-footer-outer col-12'>
                    <b:section id='Footer' maxwidgets='1' showaddelement='yes' />
                </footer>
                <!-- Designed by KITTYBBIT -->
            </div>
        </div>
    </body>
</html>