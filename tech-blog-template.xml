        <link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.12.1/css/all.css' />
        <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
        <b:if cond='data:blog.pageType in {&quot;item&quot;, &quot;static_page&quot;}'>
            <!-- clipboard.js -->
            <script async src='https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.6/clipboard.min.js' />
            <!-- hilight.js -->
            <script async src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/10.2.1/highlight.min.js" integrity="sha512-Ypjm0o7jOxAd4hpdoppSEN0TQOC19UtPAqD+4s5AlXmUvbmmS/YMxYqAqarQYyxTnB6/rqip9qcxlNB/3U9Wdg==" crossorigin="anonymous"></script>
            <script async src="//cdnjs.cloudflare.com/ajax/libs/highlightjs-line-numbers.js/2.8.0/highlightjs-line-numbers.min.js"></script>
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/10.2.1/styles/vs2015.min.css" integrity="sha512-w8aclkBlN3Ha08SMwFKXFJqhSUx2qlvTBFLLelF8sm4xQnlg64qmGB/A6pBIKy0W8Bo51yDMDtQiPLNRq1WMcQ==" crossorigin="anonymous" />
            <!-- tocbot -->
            <script async src="https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.12.0/tocbot.min.js" integrity="sha512-oD3xGN8YTxenx6tdS4D/RKqO4OtORBQtAb2/FseM17GGMIi6jMwKUBc8duX4A5RwMOGGXoFBZrsqbOk8GpXFgQ==" crossorigin="anonymous"></script>
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.12.0/tocbot.css" integrity="sha512-2kqK5bWDwdrluTn6Wkj+jgiwyzvAHLirVp08Kz7kN5I8PWQ0tUtEMxfhyWDCrnws91BJ9PfrUy6ImfA7Qb22TA==" crossorigin="anonymous" />
            <!-- mermaid.js -->
            <script async src="https://cdnjs.cloudflare.com/ajax/libs/mermaid/8.8.2/mermaid.min.js" integrity="sha512-x8NWYlEsC86ngfO24tbxW6pMuyn9gYnwEW0FcSobohDc3iLCXhmRkqYXgTfE7Jwy2YCTnHRfyum8LUIiyvmgWQ==" crossorigin="anonymous"></script>
            <!-- mathjax.js -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/mathjax/3.1.2/es5/tex-mml-chtml.min.js" integrity="sha512-7rZgZU/u5XjLaO7dBpkcvZ2+ilGXbdIak0FXUgMoO+adNy7RUceort055Wn7LkZY3VLwEsSDpi8Ui+32N1vrfw==" crossorigin="anonymous"></script>
            <!-- initialize -->
            <script type="text/javascript">
                $(function(){
                    // hilight.js
                    $('.k-item-body pre > code').addClass('k-code-height');
                    $('.k-item-body pre > code').click(function() {
                        var e = $(this);
                        if(e.hasClass('k-code-height')) {
                            e.removeClass('k-code-height');
                        } else {
                            e.addClass('k-code-height');
                        }
                    });
                    hljs.initHighlighting();
                    hljs.initLineNumbersOnLoad({
                        singleLine: true
                    });

                    // tocbot.js
                    $('.js-toc-content h1,h2,h3,h4').each(function(i, e) {
                        $(e).attr('id', i);
                    });
                    tocbot.init({
                        tocSelector: '.js-toc',
                        contentSelector: '.js-toc-content',
                        headingSelector: 'h1, h2, h3, h4',
                        hasInnerContainers: true,
                        collapseDepth: 4,
                        listItemClass: 'nav-item',
                    });
                    tocbot.init({
                        tocSelector: '.js-toc-lg',
                        contentSelector: '.js-toc-content',
                        headingSelector: 'h1, h2, h3, h4',
                        collapseDepth: 4,
                        listItemClass: 'nav-item',
                    });

                    // mermaid.js
                    mermaid.initialize({startOnLoad:true});

                    // mathjax.js
                    MathJax = {
                        tex: {
                          inlineMath: [['$', '$'], ['\\(', '\\)']]
                        }
                    };
                });
            </script>
        </b:if>
        <b:if cond='data:blog.pageType == &quot;index&quot;'>
            <title>
                <data:blog.pageTitle />
            </title>
            <meta property='og:type' content='website' />
            <b:else />
            <title>
                <data:blog.title />
            </title>
            <meta property='og:type' content='article' />
        </b:if>
        <b:if cond='data:blog.metaDescription != &quot;&quot;'>
            <meta name='description' expr:content='data:blog.metaDescription' />
        </b:if>
        <meta property='og:site_name' expr:content='data:blog.title' />
        <meta name='twitter:card' content='summary' />
        <meta name='twitter:site' content='@kittybbit' />
        <b:skin>
            <![CDATA[
                        /*
                        <Variable name="default.font.size" description="default font size" type="string" default="16px" />
                        <Variable name="base.color" description="base color" type="color" default="#fffbf2" />
                        <Variable name="accent.color" description="accent color" type="color" default="#a5c1ff" />

                        <Group description="Header">
                            <Variable name="header.text.font" description="Header Font"
                                type="font"
                                default="$(default.font.size) -apple-system,Segoe UI,Helvetica Neue,Hiragino Kaku Gothic ProN,meiryo,sans-serif"/>
                            <Variable name="header.background.color" description="Header background color"
                                type="color"
                                default="$(base.color)" />
                        </Group>
                        <Group description="Body">
                            <Variable name="body.text.font" description="Body Font"
                                type="font"
                                default="$(default.font.size) -apple-system,Segoe UI,Helvetica Neue,Hiragino Kaku Gothic ProN,meiryo,sans-serif"/>
                            <Variable name="body.background.color" description="Body background color"
                                type="color"
                                default="$(base.color)" />
                        </Group>
                        <Group description="Post">
                            <Variable name="post.text.color" description="Post text color"
                                type="color"
                                default="#000" />
                            <Variable name="post.background.color" description="Post background color"
                                type="color"
                                default="#fafafa" />
                        </Group>
                        */

                        body {
                            background: $(body.background.color);
                            font: $(body.text.font);
                        }

                        /* hilight.js */
                        .hljs {
                            opacity: 0.9;
                        }
                        .hljs-ln-numbers {
                            -webkit-touch-callout: none;
                            -webkit-user-select: none;
                            -khtml-user-select: none;
                            -moz-user-select: none;
                            -ms-user-select: none;
                            user-select: none;
                        
                            text-align: center;
                            color: #fff;
                            border-right: 1px solid #fff;
                            vertical-align: top;
                            padding-right: 5px;
                        }
                        .hljs-ln-code {
                            white-space: pre;
                        }
                        
                        /* my selector */
                        .k-header-outer {
                            background: $(header.background.color);
                        }
                        #k-search-form {
                            margin-bottom: 16px;
                            padding: 0.75rem 1rem;
                            position: absolute;
                            top: 0px;
                            right: 13px;
                            z-index: 6;
                        }
                        .k-search-text {
                            background: $(body.background.color);
                            border: none;
                            border-radius: 8px;
                            color: #333;
                            cursor: pointer;
                            opacity: 0;
                            outline: none;
                            margin-bottom: 16px;
                            padding: 0.75rem 1rem;
                            position: absolute;
                            top: 0px;
                            right: 1rem;
                            transition: all 0.5s;
                        }
                        .k-search-text:focus {
                            cursor: text;
                            opacity: 1;
                            height: 38px;
                            width: 20rem;
                            margin: 2px;
                        }
                        .k-search-button {
                            background: transparent;
                            margin-bottom: 16px;
                            padding: 0.75rem 1rem;
                            position: absolute;
                            top: 0px;
                            right: 1rem;
                            text-align: center;
                            z-index: 5;
                        }
                        .k-item, .k-index-item {
                            background: $(post.background.color);
                            box-shadow: 0 0 4px rgba(0,0,0,0.2);
                            color: $(post.text.color);
                        }
                        .k-item-header {
                            padding: 0.5rem;
                        }
                        .k-item-body {
                            background: linear-gradient(#eee 1px, transparent 1px);
                            background-size: auto 1.8rem;
                            margin: 0.4rem;
                            padding: 0.1rem;
                            color: $(post.text.color);
                            line-height: 1.8;
                        }
                        .k-item::before, .k-item::after {
                            content: '';
                            position: absolute;
                            height: 100%;
                            width: 98%;
                            z-index: -1;
                        }
                        .k-item::before {
                            background: $(post.background.color);
                            filter: brightness(97%);
                            box-shadow: 0 0 4px rgba(0,0,0,0.2);
                            left: 0px;
                            top: 2px;
                            transform: rotate(-0.5deg);
                        }
                        .k-item::after {
                            background: $(post.background.color);
                            filter: brightness(94%);
                            box-shadow: 0 0 2px rgba(0,0,0,0.2);
                            right: 0px;
                            top: -2px;
                            transform: rotate(0.5deg);
                        }
                        .k-item-body h1 {
                            font-size: 1.6rem;
                            vertical-align: baseline;
                            margin: 0.9rem 0rem;
                            padding-left: 0.5rem;
                            height: 1.8rem;
                            background: transparent;
                            border-left: solid 14px $(accent.color)
                        }
                        .k-item-body h2 {
                            font-size: 1.4rem;
                            vertical-align: baseline;
                            margin: 0.9rem 0rem;
                            padding-left: 0.5rem;
                            height: 1.8rem;
                            background: transparent;
                            border-left: solid 12px $(accent.color)
                        }
                        .k-item-body h3 {
                            font-size: 1.2rem;
                            vertical-align: baseline;
                            margin: 0.9rem 0rem;
                            padding-left: 0.5rem;
                            height: 1.8rem;
                            background: transparent;
                            border-left: solid 10px $(accent.color)
                        }
                        .k-item-body h4 {
                            font-size: 1.0rem;
                            vertical-align: baseline;
                            margin: 0.9rem 0rem;
                            padding-left: 0.5rem;
                            height: 1.8rem;
                            background: transparent;
                            border-left: solid 8px $(accent.color)
                        }
                        .k-item-body p {
                            margin: 1.8rem 0rem;
                        }
                        .k-code-height {
                            max-height: 10rem;
                        }
                        .k-point {
                            position: relative;
                            margin: 2rem 0;
                            padding: 0.5rem 1rem;
                            border: solid 3px $(accent.color);
                        }
                        .k-point .k-point-title {
                            position: absolute;
                            display: inline-block;
                            top: -1.8rem;
                            left: -3px;
                            padding: 0 0.5rem;
                            background: $(accent.color);
                            color: #ffffff;
                            font-weight: bold;
                            border-radius: 5px 5px 0 0;
                        }
                        .k-point ul {
                            list-style-type: none;
                            margin: 0;
                            padding-left: 0rem;
                        }
                        .k-point ul li::before {
                            content: '';
                            width: 0.5rem;
                            height: 0.8rem;
                            margin-right: 0.8rem;
                            border-right: 2px solid $(post.text.color);
                            border-bottom: 2px solid $(post.text.color);
                            display: inline-block;
                            transform:rotateZ(45deg);
                        }
                        .k-point p {
                            margin: 0; 
                            padding: 0;
                        }
                        .k-svg-icon {
                            height: 2.5rem;
                            width: 2.2rem;
                            margin-bottom: 8px;
                        }
                        .k-wrap {
                            line-break: anywhere;
                        }
                        .k-line-clump-3 {
                            display: -webkit-box;
                            text-overflow: ellipsis;
                            -webkit-box-orient: vertical;
                            -webkit-line-clamp: 3;
                            overflow: hidden;
                        }
                        .k-line-clump-5 {
                            display: -webkit-box;
                            text-overflow: ellipsis;
                            -webkit-box-orient: vertical;
                            -webkit-line-clamp: 5;
                            overflow: hidden;
                        }
                        .k-line-clump-8 {
                            display: -webkit-box;
                            text-overflow: ellipsis;
                            -webkit-box-orient: vertical;
                            -webkit-line-clamp: 8;
                            overflow: hidden;
                        }
                        ]]>
        </b:skin>

        <b:defaultmarkups>
            <b:defaultmarkup type='Common'>
                <b:includable id='widgetNotAvailableInPreview'>
                    <b:if cond='data:widget.type == &quot;AdSense&quot;'>
                        <div class='vertical-ad-placeholder'>
                            <span>
                                <b:message name='messages.adsGoHere' />
                            </span>
                        </div>
                        <b:else />
                        <b:include name='super.widgetNotAvailableInPreview' />
                    </b:if>
                </b:includable>
                <b:includable id='BreadCrumb'>
                    <nav aria-label='breadcrumb'>
                        <ol class='breadcrumb flex-nowrap text-truncate'>
                            <b:if cond='data:view.url != data:blog.homepageUrl'>
                                <li class="breadcrumb-item">
                                    <a expr:href='data:blog.homepageUrl' rel='nofollow'>
                                        <i aria-hidden='true' class="fas fa-home"></i>
                                    </a>
                                </li>
                                <li class="breadcrumb-item active">
                                    <data:blog.pageName />
                                </li>
                                <b:else />
                                <li class="breadcrumb-item">
                                    <i aria-hidden='true' class="fas fa-home"></i>
                                </li>
                            </b:if>
                        </ol>
                    </nav>
                </b:includable>
                <b:includable id='widget-title'>
                    <b:if cond='data:renderAsDetails'>
                        <summary>
                            <div class='collapsible-title'>
                                <h4 class='title'>
                                    <b:if cond='data:defaultTitle or data:title'>
                                        <b:eval expr='data:defaultTitle ?: data:title' />
                                        <b:else />
                                        <data:messages.widget />
                                    </b:if>
                                </h4>
                                <b:include data='{ iconClass: "chevron-down" }' name='chevronDownIcon' />
                                <b:include data='{ iconClass: "chevron-up" }' name='chevronUpIcon' />
                            </div>
                        </summary>
                        <b:elseif cond='data:defaultTitle or data:title' />
                        <h4 class='title'>
                            <b:eval expr='data:defaultTitle ?: data:title' />
                        </h4>
                    </b:if>
                </b:includable>
            </b:defaultmarkup>
